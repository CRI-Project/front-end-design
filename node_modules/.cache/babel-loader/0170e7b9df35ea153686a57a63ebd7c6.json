((to, from, next) => {\n    let getFlag = localStorage.getItem(\"Flag\");\n    let getid = localStorage.getItem(\"Userid\");\n    if (getFlag === \"isLogin\" && getid !== null) {\n        store.state.isLogin = true;\n        next();\n        if (!to.meta.isLogin) {\n            next({\n                path: '/main'\n            })\n        }\n    } else {\n        if (to.meta.isLogin) {\n            next({\n                path: '/',\n            })\n        } else {\n            next()\n        }\n    }\n});\n\nrouter.afterEach(() => {\n    window.scroll(0, 0);\n});\n\nnew Vue({\n    router,\n    store,\n    vuetify,\n    render: h => h(App)\n}).$mount('#app')\n"]}]}